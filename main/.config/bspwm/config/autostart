#!/usr/bin/env bash

run() {
	if ! pgrep -f "$1" ; then
		"$@"&
	fi
}

# autostart
# usage: run [command]

# xsettingsd
run xsettingsd

# wallpaper
run "$HOME"/.fehbg

# polybar
run "$HOME"/.config/polybar/launch

# notification
run dunst

# compositor
run picom

# autolock
run xss-lock slock

# mpd
run mpd
